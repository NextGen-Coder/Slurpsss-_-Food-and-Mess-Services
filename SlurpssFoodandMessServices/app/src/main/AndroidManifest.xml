    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SlurpssFoodAndMessServices">
        <activity
            android:name=".UserRegisterActivity"
            android:exported="true" />
        <activity
            android:name=".MessEditPackageActivity"
            android:exported="true" />
        <activity
            android:name=".MessEditDishActivity"
            android:exported="true" />
        <activity
            android:name=".UserHomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UserResetPasswordActivity"
            android:exported="true" />
        <activity
            android:name=".UserLoginActivity"
            android:exported="true" />
        <activity android:name=".MessAddPackageActivity" />
        <activity android:name=".MessPackageActivity" />
        <activity android:name=".MessAddDishActivity" />
        <activity android:name=".MessDishActivity" />
        <activity
            android:name=".MessEditProfileActivity"
            android:exported="true" />
        <activity
            android:name=".MessHomeActivity"
            android:exported="true" />
        <activity android:name=".MessLoginActivity" />
        <activity android:name=".MessResetPassword" />
        <activity android:name=".PackageDetailsActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".MainActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>